{
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 11
  },
  "rules": {
      "no-mixed-spaces-and-tabs": 0,
      "no-unused-vars":0,
      "require-atomic-updates": 0,
      "no-inner-declarations": 0,
      "no-unreachable": 0,
      "no-prototype-builtins": 0,
      "no-constant-condition": 0
  },
  "env": {
      "node": true,
      "es6": true
  }
}